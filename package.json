{
  "name": "tsrpc-browser",
  "version": "3.0.0-dev.0",
  "description": "TSRPC Client for browser",
  "main": "index.js",
  "scripts": {
    "pretest": "cd test && npm i & cd ..",
    "test": "cd test && npm test & cd ..",
    "dev": "cd test && npm run dev & cd..",
    "coverage": "nyc mocha test/**/*.test.ts && start coverage\\index.html",
    "build": "npm run build:js && npm run build:dts && npm run build:doc && cp package.json LICENSE README.md dist/",
    "build:js": "rm -rf dist && npx rollup -c",
    "build:dts": "rm -rf lib && npx tsc && npx api-extractor run --local --verbose && node scripts/removePrivate.js && rm -rf lib",
    "build:doc": "rm -rf docs/references && npx api-documenter markdown --input temp --output docs/references"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/k8w/tsrpc-browser.git"
  },
  "keywords": [
    "k8w",
    "kingworks",
    "tsrpc",
    "typescript",
    "fullstack",
    "rpc",
    "framework",
    "ajax",
    "fetch",
    "axios",
    "request",
    "tsbuffer",
    "fullts"
  ],
  "author": "k8w",
  "license": "Apache-2.0",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.1",
    "@microsoft/api-extractor": "^7.14.0",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^5.2.7",
    "chai": "^4.3.4",
    "rollup": "^2.45.2",
    "rollup-plugin-typescript2": "^0.22.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "typings": "index.d.ts",
  "dependencies": {
    "k8w-extend-native": "^1.4.5",
    "tsbuffer": "^2.0.0",
    "tsrpc-proto": "^1.3.2"
  }
}