  "scripts": {
    "build": "webpack",
    "dev": "START npm run dev:back & webpack-dev-server",
    "dev:front": "webpack-dev-server",
    "dev:back": "cd server && onchange **/*.ts -i -v -- ts-node index.ts & cd ..",
    "test": "START npm run start:back & npm run start:front",
