  "dependencies": {
    "@types/chai": "^4.2.18",
    "chai": "^4.3.4",
    "kunit": "^1.2.2",
    "ts-loader": "^9.2.3",
    "tsrpc": "^3.0.0-dev.21",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2"
  },
  "devDependencies": {
    "@types/node": "^14.17.3",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "onchange": "^7.1.0",
    "ts-node": "^9.1.1",
    "webpack-dev-server": "^3.11.2"
  }
